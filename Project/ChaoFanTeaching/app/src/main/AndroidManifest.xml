<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chaofanteaching">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="超凡家教"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".All" />
        <activity android:name=".MainActivity" />
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InfoListActivity"
            android:label="@string/title_activity_info_list" />
        <activity
            android:name=".InfoDetailActivity"
            android:label="@string/title_activity_info_detail" />
        <activity
            android:name=".AddInfoActivity"
            android:label="@string/title_activity_add_info"></activity>
    </application>

</manifest>